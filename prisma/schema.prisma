// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
model Student_Info{
  id String @id @default(auto()) @map("_id") @db.ObjectId
  firstname    String?
  middlename   String?
  lastname     String?
  birthdate    DateTime?
  gender       String?
  civilstatus  String?
  country      String?
  region       String?
  province     String?
  city         String?
  barangay     String?
  zipcode      String?
  address      String?
  hobbies      String?
  keya         String?
  keyb         String?
  createdAt  DateTime     @default(now())
  updatedAt  DateTime     @default(now())
}
model User {
  id        String        @id @default(auto()) @map("_id") @db.ObjectId
  email     String        @unique
  password  String?
  createdAt  DateTime     @default(now())
  userlevel String?       @default("User")
  shift     Int
}