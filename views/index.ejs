<!doctype html>
<html lang="en">
  <head>
    <title>Title</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  </head>
  <body>
    <div class="jumbotron"></div>
      <div class="">

        <a href="/create" class="btn btn-outline-dark">Insert Data</a>
        <table class="table">
          <tr style="text-align: center;">
            <td>Object ID</td>
            <td>First Name</td>
            <td>Middle Name</td>
            <td>Last Name</td>
            <td>Birthdate</td>
            <td>Gender</td>
            <td>civilstatus</td>
            <td>country</td>
            <td>region</td>
            <td>province</td>
            <td>city</td>
            <td>baranggay</td>
            <td>zipcode</td>
            <td>address</td>
            <td>hobbies</td>
            <td>date created</td>
            <td colspan="3">Action</td>
          </tr>
          <% students.forEach((student) => {%> 
          <tr style="text-align: center;">
            <td><%= student.id %></td>
            <td><%= student.firstname %></td>
            <td><%= student.middlename %></td>
            <td><%= student.lastname %></td>
            <td><%= student.birthdate %></td>
            <td><%= student.gender %></td>
            <td><%= student.civilstatus %></td>
            <td><%= student.country %></td>
            <td><%= student.region %></td>
            <td><%= student.province %></td>
            <td><%= student.city %></td>
            <td><%= student.barangay %></td>
            <td><%= student.zipcode %></td>
            <td><%= student.address %></td>
            <td><%= student.hobbies %></td>
            <td><%= student.createdAt %></td>
            <td><a href="/view/<%= student.id %>" class="btn btn-outline-info">View</a></td>
            <td><a href="/edit/<%= student.id %>" class="btn btn-outline-secondary">Update</a></td>
            <td>
              <form action="/delete/<%= student.id %>" method="post">
                <button type="submit" class="btn btn-outline-danger">
                  Delete
                </button>
              </form>
          </td>
          </tr>
          <% }) %>
        </table>
        
      </div>
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
  </body>
</html>