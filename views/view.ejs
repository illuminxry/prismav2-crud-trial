<!doctype html>
<html lang="en">
  <head>
    <title>Title</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  </head>
  <body>
    <div class="jumbotron">
      <h1>User Details</h1>
    </div>
    <div class="container">
      <form action="" method="" class="form-group">
        <label for="">First Name</label>
        <input type="text" name="name" class="form-control" value="<%= student.firstname %>" disabled>
        <br>
        <label for="">Middle Name</label>
        <input type="text" name="program" class="form-control" value="<%= student.middlename %>" disabled>
        <br>
        <label for="">Last Name</label>
        <input type="text" name="program" class="form-control" value="<%= student.lastname%>" disabled>
        <br>
        <label for="">Birthdate</label>
        <input type="text" name="program" class="form-control" value="<%= student.birthdate%>" disabled>
        <br>
        <label for="">Gender</label>
        <input type="text" name="program" class="form-control" value="<%= student.gender%>" disabled>
        <br>
        <label for="">Civil Status</label>
        <input type="text" name="program" class="form-control" value="<%= student.civilstatus%>" disabled>
        <br>
        <label for="">Country</label>
        <input type="text" name="program" class="form-control" value="<%= student.country%>" disabled>
        <br>
        <label for="">Region</label>
        <input type="text" name="program" class="form-control" value="<%= student.region%>" disabled>
        <br>
        <label for="">Province</label>
        <input type="text" name="program" class="form-control" value="<%= student.province%>" disabled>
        <br>
        <label for="">City</label>
        <input type="text" name="program" class="form-control" value="<%= student.city%>" disabled>
        <br>
        <label for="">Barangay</label>
        <input type="text" name="program" class="form-control" value="<%= student.barangay%>" disabled>
        <br>
        <label for="">Zip Code</label>
        <input type="text" name="program" class="form-control" value="<%= student.zipcode%>" disabled>
        <br>
        <label for="">Address</label>
        <input type="text" name="program" class="form-control" value="<%= student.address%>" disabled>
        <br>
        <label for="">Hobbies</label>
        <input type="text" name="program" class="form-control" value="<%= student.hobbies%>" disabled>
        <br>
        <a href="/" class="btn btn-outline-info">Go Back</a>
      </form>
    </div>
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
  </body>
</html>